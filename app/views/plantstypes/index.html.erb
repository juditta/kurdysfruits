<h1>Listing plantstypes</h1>

<table>
  <tr>
    <th>Plantstype</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @plantstypes.each do |plantstype| %>
  <tr>
    <td><%= plantstype.plantstype %></td>
    <td><%= link_to 'Show', plantstype %></td>
    <td><%= link_to 'Edit', edit_plantstype_path(plantstype) %></td>
    <td><%= link_to 'Destroy', plantstype, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

